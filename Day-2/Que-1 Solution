<h2>Using Javascript in Body, View Console</h2>


<script>
  var str ="How are you?";
  console.log(str + " is given string");
  console.log("Finding o's indeces in given string:");
  var n = str.indexOf("o",0);
  var i=1;
  while(n != -1){
  console.log(i + "th occurance of 'o' at index" + n +"\n");i++;
  n=str.indexOf("o",n+1);
  }
  
</script>

